{
  "name": "dota-hero-calculator-library",
  "version": "0.6.1",
  "description": "Dota 2 Hero Calculator Library",
  "main": "src/herocalc/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devilesk/dota-hero-calculator-library.git"
  },
  "author": "devilesk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devilesk/dota-hero-calculator-library/issues"
  },
  "scripts": {
    "build": "browserify -d -x 'knockout' -x 'jquery' --standalone HeroCalc src/herocalc/main.js | exorcist dist/herocalc.js.map > dist/herocalc.js",
    "prod-build": "browserify -d -x 'knockout' -x 'jquery' -p [minifyify --map herocalc.min.js.map --output dist/herocalc.min.js.map] --standalone HeroCalc src/herocalc/main.js > dist/herocalc.min.js",
    "prepublish": "npm run build && npm run prod-build"
  },
  "homepage": "https://github.com/devilesk/dota-hero-calculator-library#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browserify": "^13.1.0",
    "browserify-shim": "^3.8.12",
    "exorcist": "^0.4.0",
    "jquery": "^3.1.0",
    "knockout": "^3.4.0",
    "minifyify": "^7.3.3"
  },
  "dependencies": {},
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "knockout": "global:ko",
    "jquery": "global:$"
  }
}
